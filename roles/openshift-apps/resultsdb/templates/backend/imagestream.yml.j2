---
apiVersion: image.openshift.io/v1
kind: ImageStream
metadata:
  name: "resultsdb-api"
spec:
  tags:
  - name: latest
    importPolicy:
      scheduled: true
    from:
      kind: DockerImage
{% if env == 'staging' %}
      name: "{{ resultsdb_image }}"
{% else %}
      name: "{{ resultsdb_image }}"
{% endif %}
