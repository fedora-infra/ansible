plugin: constructed
strict: False
compose:
groups:
    iad2: ansible_domain == 'iad2.fedoraproject.org' or ansible_domain = 'stg.iad2.fedoraproject.org'
keyed_groups:
    - prefix: distro
      key: ansible_distribution

    - prefix: vmhost
      key: vmhost

    - prefix: datacenter
      key: datacenter

    - prefix: virt
      key: ansible_virtualization_role
